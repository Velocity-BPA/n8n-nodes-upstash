{
  "name": "n8n-nodes-upstash",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Upstash providing Redis, Kafka, QStash, and Vector database operations. Includes 12 resource categories and 80+ operations for serverless data management.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-upstash.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-upstash#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-upstash/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "upstash",
    "redis",
    "kafka",
    "qstash",
    "vector",
    "serverless",
    "database",
    "cache",
    "message-queue",
    "scheduling",
    "vector-database"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/UpstashApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Upstash/Upstash.node.js",
      "dist/nodes/Upstash/UpstashTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.27.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.27.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
